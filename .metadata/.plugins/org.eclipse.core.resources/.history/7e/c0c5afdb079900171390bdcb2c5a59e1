package 共享办公;
import java.text.ParseException;
import java.text.SimpleDateFormat;
import java.util.Date;
public class DateTime {

	public static void main(String[] args) throws ParseException{
	   System.out.println(date());
	   System.out.println(time("124530"));
	   }   	    
	    
	
	public static int time(String time) throws ParseException{
		Date dt = new Date();
		SimpleDateFormat matter1 = new SimpleDateFormat("yyyy-MM-dd");
		System.out.println("a5:"+matter1.format(dt));	
		System.out.println(dt);
		return 
	}
	
	public static String  date(){	 
		 Date date = new Date();;  
		    String timeSs = String.valueOf(date.getTime()); 
		   if(timeSs.length()>3){
			   timeSs = timeSs.substring(0, timeSs.length()-3);
		   }
		   return timeSs;
	}

}
