package jir;

public class tihuanhuiche {
	public static void main(String[] args){
		String a="da\nA\n";
		a = a.replaceAll("\n", "3");
		System.out.println(a);
		System.out.println(Transcoding.getURLEncoderString(a));
		System.out.println(tihuanhuiche(a));
	}
	public static String tihuanhuiche(StringBuilder str){
		if(str.indexOf("\n")>=0){
		 StringBuilder sb = new StringBuilder(str);
		System.out.println("========");
		 str=sb.replace(1,2,"f");
	}
	return str;
	
	}
}
