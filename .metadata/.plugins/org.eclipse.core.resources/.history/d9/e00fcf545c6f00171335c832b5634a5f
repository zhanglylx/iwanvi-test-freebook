package jir;

public class tihuanhuiche {
	public static void main(String[] args){
		String a="da\ncd";
		a = a.replace("\n", "\r\n");
		System.out.println(a);
		System.out.println(tihuanhuiche(a));
	}
	public static String tihuanhuiche(String str){
		if(str.indexOf("\n")>=0){
		 StringBuilder sb = new StringBuilder(str);
		System.out.println("========");
		 str=str.replace("d","f");
	}
	return str;
	
	}
}
