package jir;

public class RequestContent {
	 String request ;
	public RequestContent() {
		// TODO 自动生成的方法存根
		JirExcel cel = new JirExcel();
		
		for(int i=0;i<cel.Key.length;i++){
			request = request+
					cel.Key[i].trim()
					+"="+
					Transcoding.getURLEncoderString((cel.Values[i].trim()))//转码
							+"&";
		}
		//将request进行编码
		request=request.substring(4, request.length()-1);
	}
}
