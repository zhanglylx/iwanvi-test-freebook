package jir;

public class tihuanhuiche {
	public static void main(String[] args){
		String a="dasdfgh\nj";
		//a = a.replaceAll("\n", "4");
		System.out.println(tihuanhuiche(a));
	}
	public static String tihuanhuiche(String str){
		System.out.println(str.indexOf("\n"));
		if(str.indexOf("\n")>=0){
		 StringBuilder sb = new StringBuilder(str);
		System.out.println("========");
		 str=(sb.replace(2,3,"2")).toString();
	}
	return str;
	
	}
}
