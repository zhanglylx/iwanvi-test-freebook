package jir;

public class tihuanhuiche {
	public static void main(String[] args){
		String a="da\"\nA\n";
		//a = a.replaceAll("\n", "4");
		  System.out.println(a.replaceAll("\"", "++"));  
		  System.out.println(a.replace("\"", "++")); 
		System.out.println(tihuanhuiche(a));
	}
	public static String tihuanhuiche(String str){
		if(str.indexOf("\n")>=0){
		 StringBuilder sb = new StringBuilder(str);
		System.out.println("========");
		 str=(sb.replace(2,3,"2")).toString();
	}
	return str;
	
	}
}
