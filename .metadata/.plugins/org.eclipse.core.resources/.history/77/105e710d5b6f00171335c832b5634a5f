package jir;

public class tihuanhuiche {
	public static void main(String[] args){
		String a="da\ncd";
		System.out.println(tihuanhuiche(a));
	}
	public String tihuanhuiche(String str){
	if(str.indexOf("\n")<=0){
		 StringBuilder sb = new StringBuilder("str");
		 sb.replace(5,6, "S");
	
	}
	return str;
	
	}
}
